{
  "name": "api_node",
  "description": "api creada con nodejs",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {},
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 22.18.0"
  },
  "feathers": {
    "language": "js",
    "packager": "npm",
    "database": "mongodb",
    "framework": "express",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "json"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "main": "lib/index",
  "type": "module",
  "scripts": {
    "start": "node src",
    "dev": "nodemon src/",
    "prettier": "npx prettier \"**/*.js\" --write",
    "test": "jest --testTimeout=10000",
    "test:mocha": "cross-env NODE_ENV=test mocha test/ --recursive --exit",
    "bundle:client": "npm pack --pack-destination ./public",
    "test:queue": "jest test/services/queue/queue.test.js",
    "test:queue-hook": "jest test/hooks/send-to-queue.test.js",
    "test:queue-integration": "jest test/services/queue/queue-integration.test.js",
    "test:queue-schema": "jest test/services/queue/queue-schema.test.js",
    "test:queue-all": "jest test/services/queue/queue*.test.js test/hooks/send-to-queue.test.js"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.34",
    "@feathersjs/authentication": "^5.0.34",
    "@feathersjs/authentication-client": "^5.0.34",
    "@feathersjs/configuration": "^5.0.34",
    "@feathersjs/errors": "^5.0.34",
    "@feathersjs/express": "^5.0.34",
    "@feathersjs/feathers": "^5.0.34",
    "@feathersjs/mongodb": "^5.0.34",
    "@feathersjs/schema": "^5.0.34",
    "@feathersjs/socketio": "^5.0.34",
    "@feathersjs/transport-commons": "^5.0.34",
    "amqplib": "^0.10.9",
    "compression": "^1.8.1",
    "joi": "^18.0.1",
    "mongodb": "^6.16.0",
    "node-fetch": "^3.3.2",
    "pdfkit": "^0.17.1",
    "pdfmake": "^0.2.20",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@feathersjs/cli": "^5.0.34",
    "@feathersjs/rest-client": "^5.0.34",
    "@jest/globals": "^30.1.1",
    "axios": "^1.9.0",
    "babel-jest": "^30.1.1",
    "cross-env": "^10.0.0",
    "jest": "^30.1.1",
    "mocha": "^11.2.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2"
  }
}
